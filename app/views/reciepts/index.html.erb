<p id="notice"><%= notice %></p>

<h1>Reciepts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reciepts.each do |reciept| %>
      <tr>
        <td><%= link_to 'Show', reciept %></td>
        <td><%= link_to 'Edit', edit_reciept_path(reciept) %></td>
        <td><%= link_to 'Destroy', reciept, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reciept', new_reciept_path %>
